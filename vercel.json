{
  "buildCommand": "echo 'Static'",
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/proxy/:path*",
      "destination": "/api/proxy/:path*"
    },
    {
      "source": "/s=:query",
      "destination": "/index.html"
    },
    {
      "source": "/player/:path*",
      "destination": "/player.html"
    }
  ]
}
