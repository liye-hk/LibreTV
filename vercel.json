{
  "buildCommand": "npm install",
  "outputDirectory": ".",
  "headers": [
    {
      "source": "/js/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/css/:path*", 
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/js/:path*",
      "destination": "/js/:path*"
    },
    {
      "source": "/css/:path*",
      "destination": "/css/:path*"
    },
    {
      "source": "/libs/:path*",
      "destination": "/libs/:path*"
    },
    {
      "source": "/proxy/:path*",
      "destination": "/api/proxy/:path*"
    },
    {
      "source": "/s=:query",
      "destination": "/index.html"
    },
    {
      "source": "/player.html",
      "destination": "/player.html"
    },
    {
      "source": "/player.html/:path*",
      "destination": "/player.html"
    }
  ]
}
